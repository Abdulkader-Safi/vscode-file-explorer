{
  "name": "file-explorer",
  "displayName": "file explorer",
  "description": "file explorer in vscode",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.105.0"
  },
  "publisher": "AbdulkaderSafi",
  "author": {
    "name": "Safi",
    "email": "safi.abdulkader@gmail.com",
    "url": "https://abdulkadersafi.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Abdulkader-Safi/vscode-file-explorer"
  },
  "icon": "./media/image.png",
  "keywords": [
    "code explanation",
    "developer tools"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "file-explorer.helloWorld",
        "title": "File Explorer"
      }
    ],
    "viewsContainers": {},
    "views": {}
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test",
    "package:run": "vsce package",
    "publish": "vsce publish"
  },
  "dependencies": {
    "ssh2": "^1.15.0",
    "ssh2-sftp-client": "^10.0.3"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/ssh2": "^1.15.0",
    "@types/ssh2-sftp-client": "^9.0.5",
    "@types/vscode": "^1.105.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.10",
    "eslint": "^9.36.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3"
  }
}
